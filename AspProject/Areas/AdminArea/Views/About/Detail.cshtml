@model About

@{
    ViewData["Title"] = "Edit";
}

<div class="container mt-5" style="text-align:center">
    <h4>About</h4>
    <img src="~/img/about/@Model.Image" class="mt-3" style="width:300px; height:150px;" />
    <h5 class="mt-5">@Model.Title</h5>
    <p>@Model.Description</p>
    <div>
        <a asp-action="Index" class="btn btn-primary">Go back</a>
    </div>
</div>